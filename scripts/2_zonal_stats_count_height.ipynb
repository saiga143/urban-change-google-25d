    "plt.figure(figsize=(8,4))\n",
    "plt.plot(timestamps, processing_speeds, marker='o')\n",
    "plt.title(\"Processing Speed Over Time\")\n",
    "plt.xlabel(\"Time Elapsed (s)\")\n",
    "plt.ylabel(\"Grids Processed / sec\")\n",
    "plt.grid(True)\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  }
